{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://tayyaba98-full-stack-web-application.hf.space/api/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://tayyaba98-full-stack-web-application.hf.space"
  }
}
